    text-align: center;
    font-size: 15px;
    font-weight: bold;
    line-height: 1.3;
    height: 70px;
  }
  .gnav .gnav_sub_item a span {
    font-size: 14px;
  }
  .gnav .right a span {
    height: 90px;
    width: 100px;
  }
  .gnav_estate,
  .gnav_reservation {
    position: absolute;
    top: 0;
    width: 110px;
  }
  .gnav_estate {
    right: 110px;
  }
  .gnav_reservation {
    right: 0;
  }

  .gnav_estate a {
    color: #fff;
    background: #f1be24;
  }

  .gnav_estate a:hover {
    background: #f9d200;
  }

  .gnav_reservation a {
    color: #fff;
    background: #f27b2c;
  }

  .gnav_reservation a:hover {
    background: #F28F2C;
  }
  .gnav_sub {
    display: none;
  }
  .gnav_sub_unorderded {
    position: absolute;
    left: 0;
    top: 100%;
    width: 100%;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    background: rgba(255,255,255,0.9);
    border-top: 1px solid #ddd;
  }

  .is-fixed {
    position: fixed;
