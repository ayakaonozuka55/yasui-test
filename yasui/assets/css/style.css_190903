  .gnav .gnav_item.hover .gnav_link::before {
    opacity: 1;
  }
  .gnav .gnav_link_arrow {
    padding-right: 12px;
  }
  .gnav .gnav_link_arrow::after {
    position: absolute;
    content: '';
    border-right: 2px solid #000;
    border-bottom: 2px solid #000;
    width: 4px;
    height: 4px;
    -webkit-transition: all .5s;
    transition: all .5s;
    top: 48%;
    right: 0;
    -webkit-transform: translateY(-50%) rotate(45deg);
    -ms-transform: translateY(-50%) rotate(45deg);
    transform: translateY(-50%) rotate(45deg);
  }
  .gnav .gnav_item {
    margin: 0 10px;
  }
  .gnav_sub_item {
    margin: 0 15px;
  }
  .gnav_sub_item_top {
    display: none;
  }
  .gnav .gnav_item a span,
